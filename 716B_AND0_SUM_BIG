#include <bits/stdc++.h>
using namespace std;
#define ll long long 
#define pb push_back
#define mod 1000000007
int main(){
	
		int T;
		cin >> T;
		while(T--){
			ll n , k;
			cin >> n >> k;
			ll ans = 1;
			for(ll i = 0 ; i < k ; i++){
				ans = (ans * n) % mod;
			}
	
			cout << ans << endl;
	
	}
	return 0;
}